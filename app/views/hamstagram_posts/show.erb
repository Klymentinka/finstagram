
<header>
<h1>Hamstagram</h1>
<div class="login-info">
  <% if current_user %>
    <%= current_user.username %>
    <a href="/logout">Logout</a>
  <% else %>
    <a href="/login">Login</a>
    <a href="/signup">Signup</a>
  <% end %>
</div>
</header>
<main role="main">
    <%= erb(:'shared/hamstagram_post', { locals: { hamstagram_post: @hamstagram_post }}) %>
</main>
